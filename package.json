{
  "name": "dd",
  "version": "0.0.1",
  "scripts": {
    "start": "node my-output-directory",
    "dev": "svelte-kit dev",
    "build": "svelte-kit build",
    "package": "svelte-kit package",
    "preview": "svelte-kit preview",
    "prepare": "npm run vite-workaround && npm run smui-theme-light && npm run smui-theme-dark",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
    "vite-workaround": "node _helper/vite-workaround.js",
    "smui-theme-light": "smui-theme compile static/smui.css -i src/theme",
    "smui-theme-dark": "smui-theme compile static/smui-dark.css -i src/theme/dark"
  },
  "devDependencies": {
    "@material/typography": "^13.0.0",
    "@mdi/js": "^6.5.95",
    "@smui-extra/accordion": "^6.0.0-beta.11",
    "@smui/button": "^6.0.0-beta.8",
    "@smui/card": "^6.0.0-beta.11",
    "@smui/chips": "^6.0.0-beta.11",
    "@smui/icon-button": "^6.0.0-beta.8",
    "@smui/list": "^6.0.0-beta.11",
    "@smui/top-app-bar": "^6.0.0-beta.8",
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/adapter-node": "^1.0.0-next.58",
    "@sveltejs/kit": "next",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^3.2.1",
    "prettier": "^2.4.1",
    "prettier-plugin-svelte": "^2.4.0",
    "smui-theme": "^5.0.0",
    "svelte": "^3.44.0",
    "svelte-preprocess": "^4.10.1",
    "typescript": "^4.5.4"
  },
  "type": "module",
  "dependencies": {
    "@fontsource/fira-mono": "^4.5.0",
    "@lukeed/uuid": "^2.0.0",
    "cookie": "^0.4.1"
  }
}
